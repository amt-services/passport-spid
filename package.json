    "cheerio": "^1.0.0-rc.12",
    "cssesc": "^3.0.0",
    "fast-xml-parser": "^4.0.8",
    "xml-crypto": "^2.1.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/cssesc": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.3",
    "@types/node": "^16.0.0",
    "@types/xml-crypto": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
